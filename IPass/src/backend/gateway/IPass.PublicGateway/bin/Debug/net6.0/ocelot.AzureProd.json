{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "webapp-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "App"
    },
    {
      "DownstreamPathTemplate": "/{catchAll}",
      "DownstreamScheme": "ws",
      "DownstreamHostAndPorts": [
        {
          "Host": "notification-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    },
    {
      "DownstreamPathTemplate": "/chat/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "notification-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/chat/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    },
    {
      "DownstreamPathTemplate": "/notification/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "notification-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/notification/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    },
    {
      "DownstreamPathTemplate": "/identity/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authentication-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/identity/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "Auth"
    },
    {
      "DownstreamPathTemplate": "/signin-google",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authentication-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/signin-google",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "Auth"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "App",
      "Config": [
        {
          "Name": "Koctas app API proxy",
          "Version": "v1",
          "Url": "http://webapp-api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "Auth",
      "Config": [
        {
          "Name": "Koctas Auth proxy",
          "Version": "v1",
          "Url": "http://authentication-api/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7001/"
  }
}
